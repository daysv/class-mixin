{
  "name": "class-mixin",
  "version": "1.0.3",
  "description": "Mixin for ES6 classes",
  "main": "./lib",
  "scripts": {
    "compile": "./node_modules/.bin/babel -d lib/ src/",
    "mocha": "./node_modules/.bin/mocha --compilers js:babel-core/register",
    "test": "npm run compile && npm run mocha",
    "cover": "./node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register",
    "codecov": "npm run cover && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daysv/class-mixin.git"
  },
  "keywords": [
    "class",
    "mixin",
    "mix",
    "es6",
    "es2015"
  ],
  "author": "sang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daysv/class-mixin/issues"
  },
  "homepage": "https://github.com/daysv/class-mixin#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "codecov": "^1.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5"
  }
}
